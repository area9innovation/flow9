Behaviour<?> ::= ConstBehaviour<?>, DynamicBehaviour<?>;

	// The runtime representation of a behaviour
	DynamicBehaviour(value : ref ?);
	ConstBehaviour : (constv : ?);

Transform<?> ::= ConstBehaviour<?>, DynamicBehaviour<?>, FSelect<?>, FSelect2<?>, FSubSelect<?>;
	FSelect(b : Transform<flow>);
	FSelect2(b1 : Transform<flow>, b2 : Transform<flow>);
	FSubSelect(b : Transform<flow>);

