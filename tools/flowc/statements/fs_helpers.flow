fsHasTailCall(s: FsStat) -> bool {
	switch (s) {
		FsIf(e, s1, s2,__): {
			fsHasTailCall(s1) || fsHasTailCall(s2);
		}
		FsSwitch(v,__, cases,__): {
			exists(cases, \c -> fsHasTailCall(c.body));
		}
		FsSeq(ss,__): {
			if (length(ss) == 0) false else fsHasTailCall(ss[length(ss) - 1]);
		}
		FsLet(__,__,s1,__): {
			fsHasTailCall(s1);
		}
		FsRet(e,__): {
			switch (e) {
				FsTailCall(__,__,__,__): true;
				default: false;
			}
		}
		default: false;
	}
}

