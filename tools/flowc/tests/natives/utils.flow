import runtime;
export {
    Equal(a: ?, b: ?, msg: string) -> bool;
    NotEqual(a: ?, b: ?, msg: string) -> bool;
}

Equal(a: ?, b: ?, msg: string) -> bool {
    if (a == b) {
        println(msg + " OK");
        true
    } else {
        println("Failed: " + msg);
        false;
    }
}

NotEqual(a: ?, b: ?, msg: string) -> bool {
    if (a != b) {
        println(msg + " OK");
        true
    } else {
        println("Failed: " + msg);
        false;
    }
}
