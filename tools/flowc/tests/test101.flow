import tools/flowc/fcmodule;

foo(module) {
	gvar = lookupTree(module.globalVars, "");
	mexp : Maybe<FcExp> = switch (gvar) {
		None(): {
			gfn = lookupTree(module.functions, "");
			switch (gfn) {
				None(): None();
				Some(fn): fn.lambda;
			}
		}
		Some(gv): gv.value;
	}
	fv = mfreevars(mexp);
}

mfreevars(m : Maybe<FcExp>) -> Set<string> {
	makeSet();
}
