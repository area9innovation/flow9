import runtime;
import string;

// inlining of constant global varibles.

X : (s : string, a : A);
A : (s : string, a : B);
B : (s : string, a : C);
C : (s : string, a : D);
D : ();

str_ = "str"
a = 5;
b = 1.2;
// This string should be inlined
c = "12345";
d = true;
// This string is too long to be inlined
e = "123456";
// inlining of nested struct strings
f = X("a", A("b", B("c", C("d", D()))));

main() {
	println(i2s(a));
	println(d2s(b));
	println(c);
	println(b2s(d));
	println(e);
	println(f.a.a.s);
	println(str_);
}

