// testing tree 

Union5 ::= Struct51, Struct52;
	Struct51(i : int);
	Struct52(i : int);

test5() {
	a = [Struct51(51), Struct52(52)];
	tr = fold(a, makeTree(), \acc, s -> {
		setTree(acc, i2s(s.i), s)
	});
	f = \x -> {
		m = lookupTree(tr, x);
		switch (m) {
			Some(s): s.i;
			None(): 0;
		}
	}
	check3(f("51") == 51, "test5.1");
	check3(f("52") == 52, "test5.2");
}

