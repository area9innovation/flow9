import string_utils;

fn1(x: string, y: string, descr: string) -> void {
	x1 = "\"" + x + "\"";
	println(x + ": " + y + " " + if (x1 == y) "ok" else "NOX - " + descr);
}

fn(x: string) -> void {
	fn1(x, toStringFormatted(x), "toStringFormatted");
	fn1(x, toString(x), "toString");
}

