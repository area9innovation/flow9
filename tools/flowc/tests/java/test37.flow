import string;

// Nested-interleaved with calls constant struct field access.

Struct1 : (a : int, b : int);
Struct2 : (s : Struct1);
Struct3 : (s : Struct2);

fun1(s : Struct1) -> int {
	s.a + s.b;
}

fun2(s : Struct2) -> Struct1 {
	s.s
}

fun3(s : Struct3) -> Struct2 {
	s.s
}

main() {
	s = Struct3(Struct2(Struct1(1, 2)));
	println(fun1(fun2(fun3(s))));
}


