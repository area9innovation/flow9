import string;

fn(i: int, acc: Pair<string, int>, s: ref string) -> Pair<string, int> {
	if (i == 0) acc else {
		s := acc.first + "+" + i2s(i);
		s := ^s + "AAA";
		fn(i - 1, Pair(acc.first + " " + i2s(i), acc.second + 1), s);
	}
}

fn1(i: int, s: ref string) -> string {
	if (i == 0) ^s else {
		s := "+" + i2s(i) + ^s;
		s := ^s + "AAA";
		fn1(i - 1, s);
	}
}

