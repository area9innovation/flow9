"template<typename T> T $F_1(for)(T v, Fun<Bool, T>* p, Fun<T, T>* fn);\n",
"template<typename T> T $F_1(for)(T v, Fun<Bool, T>* p, Fun<T, T>* fn) { 
	while (true) {
		incRc(v);
		if (!p->callRc1(v)) {
			break;
		}
		v = fn->callRc1(v);
	}
	decRc(p);
	decRc(fn);
	return v;
}\n\n", "", [], []),

