Cpp3NativePlain("filter", true,
"template<typename T> Vec<T>* $F_1(filter)(Vec<T>* a, Fun<Bool, T>* fn);\n",
"template<typename T>
Vec<T>* $F_1(filter)(Vec<T>* a, Fun<Bool, T>* fn) {
	if (isUnitRc(a)) {
		std::vector<T>& vec = a->vecRef();
		Int j = 0;
		for (Int i = 0; i < a->size(); ++ i) {
			T x = a->getRc1(i);
			if (fn->callRc1(x)) {
				if (i != j) {
					vec[j] = vec[i];
				}
				j += 1;
			} else {
				decRc(x);
			}
		}
		vec.resize(j);
		decRc(fn);
		return a;
	} else {
		Vec<T>* ret = Vec<T>::make(a->size());
		for (T x : *a) {
			incRc(x);
			if (fn->callRc1(x)) {
				incRc(x);
				ret->pushBack(x);
			}
		}
		decRc(fn);
		decRc(a);
		//ret->vecRef().shrink_to_fit();
		return ret;
	}
}\n", "", [], []),

Cpp3NativePlain("replace", true,
"template<typename T> Vec<T>* $F_1(replace)(Vec<T>* a, Int i, T v);\n",
"template<typename T>
Vec<T>* $F_1(replace)(Vec<T>* a, Int i, T v) {
	Vec<T>* ret = nullptr;
	if (isUnitRc(a)) {
		ret = a;
	} else {
		ret = Vec<T>::make(*a);
		decRc(a);
	}
	if (0 <= i && i < ret->size()) {
		ret->setRc1(i, v);
	} else if (i == ret->size()) {
		ret->pushBack(v);
	} else {
		decRc(v);
	}
	return ret;
}\n", "", [], []),

Cpp3NativePlain("subrange", true,
"template<typename T> Vec<T>* $F_1(subrange)(Vec<T>* a, Int i, Int len);\n",
"template<typename T>
Vec<T>* $F_1(subrange)(Vec<T>* a, Int i, Int len) {
	Int size = a->size();
	if (i < 0 || len < 1 || i >= size) {
		decRc(a);
		return Vec<T>::make();
	} else {
		Int end = i + len;
		if (end > size || end < 0) {
			len = size - i;
		}
		if (isUnitRc(a)) {
			std::vector<T>& vec = a->vecRef();
			for (Int k = 0; k < size; ++ k) {
				if (k < i || k >= i + len) {
					decRc(vec[k]);
				} else {
					vec[k - i] = vec[k];
				}
			}
			vec.resize(len);
			return a;
		} else {
			Vec<T>* ret = Vec<T>::make(len);
			for (Int j = 0; j < len; ++ j) {
				T x = a->get(i + j);
				incRc(x);
				ret->pushBack(x);
			}
			decRc(a);
			return ret;
		}
	}
}\n", "", [], []),

