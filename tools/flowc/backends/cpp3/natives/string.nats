Cpp3NativePlain("substring", false,
"String* $F_1(substring)(String* s, Int i, Int l);\n",
"String* $F_1(substring)(String* s, Int i, Int l) {
	Int strlen = static_cast<Int>(s->str.size());
	Int start = i;
	Int len = l;
	if (len < 0) {
		if (start < 0) {
			len = 0;
		} else {
			Int smartLen1 = len + start;
			if (smartLen1 >= 0) {
				len = 0;
			} else {
				Int smartLen2 = smartLen1 + strlen;
				if (smartLen2 <= 0) len = 0;
				else len = smartLen2;
			}
		}
	}
	if (start < 0) {
		Int smartStart = start + strlen;
		if (smartStart > 0) {
			start = smartStart;
		} else {
			start = 0;
		}
	} else if (start >= strlen) {
		len = 0;
	}
	if (len < 1) {
		decRc(s);
		return String::make();
	} else {
		Int end = start + len;
		if (end > strlen || end  < 0) {
			len = strlen - start;
		}
		String* ret = String::make(s->str.substr(start, len));
		decRc(s);
		return ret;
	}
}\n", "", [], []),

