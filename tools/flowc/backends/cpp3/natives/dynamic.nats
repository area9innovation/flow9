		case TypeFx::BOOL:   return 1;
		case TypeFx::DOUBLE: return 3;
		case TypeFx::STRING: return 4;
		case TypeFx::ARRAY:  return 5;
		case TypeFx::REF:    return 31;
		case TypeFx::FUNC:   return 34;
		case TypeFx::NATIVE: return 32;
