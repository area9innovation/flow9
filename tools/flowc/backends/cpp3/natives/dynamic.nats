Cpp3NativePlain("extractStructArguments", true,
"Vec<Flow> $F_1(extractStructArguments)(Flow f);\n",
"inline Vec<Flow> $F_1(extractStructArguments)(Flow f) {
	if (f->isStruct()) {
		PStr str = f->toAStr();
		std::vector<Flow> ret;
		ret.reserve(str->size());
		for (Int i = 0; i < str->size(); ++ i) {
			Flow field = str->getFlowField(i);
			field->incRefs();
			ret.push_back(field);
		}
		f->checkRefs();
		return Vec<Flow>::make(std::move(ret));
	} else {
		return Vec<Flow>::make();
	}
}\n", "", [], []),

Cpp3NativePlain("extractStructName", true,
"String $F_1(extractStructName)(Flow f);\n",
"inline String $F_1(extractStructName)(Flow f) {
	if (f->isStruct()) {
		String name = f->toAStr()->name();
		f->checkRefs();
		return name;
	} else {
		f->checkRefs();
		return String::make();
	}
}\n", "", [], []),

Cpp3NativePlain("runtimeValueType", true,
"String $F_1(runtimeValueType)(Flow f);\n",
"inline String $F_1(runtimeValueType)(Flow f) {
	switch (f->type()) {
		case Type::INT:    f->checkRefs(); return String::make(u\"int\");
		case Type::BOOL:   f->checkRefs(); return String::make(u\"bool\");
		case Type::DOUBLE: f->checkRefs(); return String::make(u\"double\");
		case Type::STRING: f->checkRefs(); return String::make(u\"string\");
		case Type::ARRAY:  f->checkRefs(); return String::make(u\"array\");
		case Type::REF:    f->checkRefs(); return String::make(u\"ref\");
		case Type::FUNC:   f->checkRefs(); return String::make(u\"function\");
		case Type::NATIVE: f->checkRefs(); return String::make(u\"native\");
		default:           f->checkRefs(); return f->toAStr()->name();
	}
}\n", "", [], []),

