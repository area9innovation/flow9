import tools/dsl/registry/dsl_populate;

main() {
	populateDsls("languages");
	println(prettyDsl(evalDslProgram(makeDslEnv(), "lambda+assign_operators", << 
		x = 3; 
		println("x = " + x);
		x += 1;
		println("x +=1: " + x);
		x -= 2;
		println("x -=2: " + x);
		x *= 3;
		println("x *=3: " + x);
		x /= 2;
		println("x /=2: " + x);
		x %= 2;
		println("x %=2: " + x);
		x;
	>>)));

	println("Done");
	quit(0);
}
