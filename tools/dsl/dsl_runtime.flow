import tools/dsl/dsl_api;

export {
	defineDslRuntime(language : DslLanguage, names : [Pair<string, string>]) -> Tree<string, DslAst>;
}

defineDslRuntime(language : DslLanguage, names : [Pair<string, string>]) -> Tree<string, DslAst> {
	pairs2tree(map(names, \p -> {
		Pair(p.first, parseDsl(p.first, language, p.second))
	}));
}
