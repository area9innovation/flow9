native println2 : io (?) -> void = Native.println;
native i2s : (int) -> string = Native.i2s;

changeVal(val : int) -> int {
	val * 20
}

mapval(val : ?, fn1 : (?) -> ??, fn2 : (??) -> ???) -> ??? {
	fn2(fn1(val))
}

main() {
	println2(mapval(10, changeVal, i2s));
	println2(mapval(10, \v -> v + 1, \v2 -> "res " + i2s(v2)));
}
