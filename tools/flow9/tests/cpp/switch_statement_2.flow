native println : io (?) -> void = Native.println;

Maybe ::= None, Some;

None();
Some();

AnyStruct ::= None, Some, Any;
Any();

main() {
	m : Maybe = Some();
	switch (m) {
		Some(): println("some");
		None(): println("none");
	} // some

	a : AnyStruct = Some();
	switch (a) {
		Some(): println("some");
		None(): println("none");
		Any(): println("any");
	} // some

