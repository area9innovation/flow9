native println : io (?) -> void = Native.println;

Maybe ::= None, Some;

None();
Some();

getStruct(type : int) -> Maybe {
	if (type > 1) Some() else None()
}

main() {

	n : Maybe = if (1 > 0) Some() else None();
	println(switch (n) {
		Some(): "some";
		None(): "none";
	}); // some

	s1 = if (1 > 0) "+" else "-";
	println(s1); // +

	println(switch (getStruct(1)) {
		Some(): "some fn";
		None(): "none fn";
	}); // none fn
}
