FlowStruct(val : flow);


Maybe<?> ::= None, Some<?>;
	None();
	Some(value : ?);

flow2s(f : flow) -> string {
	cast(f : flow -> string);
}

isSome(m : Maybe<?>) -> bool {
	switch (m : Maybe) {
		None(): false;
		Some(__): true;
	}
}

testCast(a : [?], test : (?) -> Maybe<??>) -> [??] {
	mapped : [Maybe<??>] = map(a, test);
	filtered = filter(mapped, isSome);
	map(filtered, \v : Maybe<??> ->
		cast(v : Maybe<??> -> Some<??>).value
	);
}

