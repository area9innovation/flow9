native println : io (?) -> void = Native.println;

Maybe ::= None, Some;

None();
Some(val : int);

AnyStruct ::= None, Some, Any;
Any();

main() {
	unionVar : Maybe = Some(11);
	switch (unionVar) {
		None(): println("none");
		Some(v): println("some");
	} // some

	structVar = Some(12);

	a : AnyStruct = Some(55);
	switch (a) {
		Any(): println("any");
		Some(v): println("some");
		None(): println("none");
	} // some

}
