native println2: io (?) -> void = Native.println;
idfn(x) x;

fori(start : int, end : int, fn : (i : int) -> void) -> void {
    if (start <= end) {
        fn(start);
        fori(start + 1, end, fn);
    }
}

nop() -> void {}

main() {
    fori(2, 8, \i -> {
        {}
    });
    fori(2, 7, \i -> {
        println2(10 + i);
        idfn(i);
        {}
    });

     fori(2, 8, \i -> {
        2;
        nop()
    });
    fori(2, 8, \i -> {
        println2(i);
        if (i < 5) {{}} else {{}};
    });
}
