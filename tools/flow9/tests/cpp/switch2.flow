native println : io (?) -> void = Native.println;

Maybe ::= None, Some;

None();
Some();

AnyStruct ::= None, Some, Any;
Any();

main() {
	m : Maybe = Some();
	println(switch (m) {
		Some(): "some";
		None(): "none";
	}); // some

	a : AnyStruct = Some();
	println(switch (a) {
		Some(): "some";
		None(): "none";
		Any(): "any";
	}); // some

	n = if (1 > 0) Some() else None();
	println(switch (n) {
		Some(): "some";
		None(): "none";
	}); // some

	println(switch (if (1 < 0) Some() else None()) {
		Some(): "some";
		None(): "none";
		Any(): "any";
	}); // some

	println(switch (if (1 < 0) Some() else None()) {
		Some(): "some";
		None(): "none";
	}); // none

	arr = [Some(), None()];
	println(switch (arr[1]) {
		Some(): "some";
		None(): "none";
	}); // none
}
