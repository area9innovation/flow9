Struct(v : int);

// ------------------------------------------------------------------------------------------
// TODO: fix 'int& start'
fn1(start : int, end : int) -> int {
	if (start >= end) {
		start
	} else {
		fn1(start + 1, end)
	}
}
// ------------------------------------------------------------------------------------------
fn2(start : Struct, end : Struct) -> Struct {
	if (start.v >= end.v) {
		start
	} else {
		fn2(Struct(start.v + 1), end)
	}
}
