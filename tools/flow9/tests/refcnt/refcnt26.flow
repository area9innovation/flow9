fn1(v : ?, fn : (?) -> ??) -> ?? {
	fn(v);
}
changeInt(val : int) -> int { val * 20 }
changeStruct(val : Struct1) -> Struct1 { Struct1(val.i * 20); }
foo1() -> void {
	fn1(10, changeInt);
	fn1(Struct1(10), changeStruct);
	fn1("st", strlen);
	// TODO: lambda
}
