#!/bin/bash

set -e

SCRIPT_DIR=$( cd "$( dirname "$0" )" && pwd -P )

BASE_DIR=$( cd "$( dirname "$SCRIPT_DIR" )" && pwd -P )
DEBUG=$BASE_DIR/debug

exec java -Xms128m -Xmx1024m -Xss4m -classpath "$DEBUG/clojure-1.2.0.jar:$DEBUG/clojure-contrib-1.2.0.jar:$DEBUG" clojure.main "$DEBUG/flowprof.clj" "$@"
