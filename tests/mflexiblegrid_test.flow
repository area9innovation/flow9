import material/material_ui;

main() {
	manager = makeMaterialManager([]);
	minWidth = 400.0;
	maxWidth = 600.0;

	m =
		MFlexibleGrid(
			const([
				TRectangle([Fill(red)], TFillXYXY(minWidth, 40.0, maxWidth, 40.0))
				|> TDebugMetricsRed,
				TRectangle([Fill(green)], TFillXYXY(minWidth, 40.0, maxWidth, 40.0))
				|> TDebugMetricsGreen,
				TRectangle([Fill(blue)], TFillXYXY(minWidth, 40.0, maxWidth, 40.0))
				|> TDebugMetricsBlue,
				TRectangle([Fill(red)], TFillXYXY(minWidth, 40.0, maxWidth, 40.0))
				|> TDebugMetricsRed,
				TRectangle([Fill(green)], TFillXYXY(minWidth, 40.0, maxWidth, 40.0))
				|> TDebugMetricsGreen,
				TRectangle([Fill(blue)], TFillXYXY(minWidth, 40.0, maxWidth, 40.0))
				|> TDebugMetricsBlue,
				TRectangle([Fill(red)], TFillXYXY(minWidth, 40.0, maxWidth, 40.0))
				|> TDebugMetricsRed,
				TRectangle([Fill(green)], TFillXYXY(minWidth, 40.0, maxWidth, 40.0))
				|> TDebugMetricsGreen,
				TRectangle([Fill(blue)], TFillXYXY(minWidth, 40.0, maxWidth, 40.0))
				|> TDebugMetricsBlue,
				TRectangle([Fill(red)], TFillXYXY(minWidth, 40.0, maxWidth, 40.0))
				|> TDebugMetricsRed,
				TRectangle([Fill(green)], TFillXYXY(minWidth, 40.0, maxWidth, 40.0))
				|> TDebugMetricsGreen,
				TRectangle([Fill(blue)], TFillXYXY(minWidth, 40.0, maxWidth, 40.0))
				|> TDebugMetricsBlue,
				TRectangle([Fill(red)], TFillXYXY(minWidth, 40.0, maxWidth, 40.0))
				|> TDebugMetricsRed,
				TRectangle([Fill(green)], TFillXYXY(minWidth, 40.0, maxWidth, 40.0))
				|> TDebugMetricsGreen,
				TRectangle([Fill(blue)], TFillXYXY(minWidth, 40.0, maxWidth, 40.0))
				|> TDebugMetricsBlue
			]
			|> (\f -> generate(0, 2, \__ -> f) |> concatA)),
			[TSameSize()]
		)
		|> MDebugMetricsPurple;

	mrender(manager, true, m);
}