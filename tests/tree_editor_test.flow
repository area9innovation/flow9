import material/extra/tree_editor/tree_editor;

main() {
	manager = makeMaterialManager([]);
	getNewId = \cb -> {
		cb(makeTropicTag(manager));
	}
	getNewContent = \cb -> {
		cb(TEmpty());
	}
	tree = makeMTreeEditorNode(makeTropicTag(manager), TEmpty());
	editor = makeMTreeEditor(manager, tree, getNewId, getNewContent, []);

	mrender(
		manager,
		true,
		MLines2(
			editor.toolbar,
			editor.view
		)
	)
	|> ignore;
}