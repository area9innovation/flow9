import material/extra/tree_editor/tree_editor;

main() {
	manager = makeMaterialManager([]);
	getNewNode = \ -> makeMTreeEditorNodeDefault(manager);
	tree = makeMTreeEditorNodeDefault(manager);
	editor = makeMTreeEditor(manager, tree, \cb -> cb(getNewNode()), [MKeysEnabled(const(false))]);

	mrender(
		manager,
		true,
		MLines2InvertZorder(
			editor.toolbar,
			editor.view
		)
	)
	|> ignore;
}