import material/material_ui;

main() {
    setRendererType("html");
    manager = makeMaterialManager([]);

    images = [
        "http://thefoxisblack.com/desktop-wallpaper/denise-nouvion-wallpapers-4-1280x800.jpg",
        "http://thefoxisblack.com/desktop-wallpaper/adrian-zorzano-wallpaper-1280x800.jpg",
        "http://thefoxisblack.com/desktop-wallpaper/michael-olivo-wallpaper-1280x800.jpg",
        "http://thefoxisblack.com/desktop-wallpaper/Mike-Dornseif-wallpaper-1280x800.jpg"
    ];

    ShowMPrintPagesDialog(manager, map(enumFromTo(0, 100), \i ->
        TZoomToFill(
            TPicture(images[floor(random() * i2d(length(images)))], []),
            TRectangle([Fill(if (i % 2 == 0) blue else green), Stroke(red), StrokeWidth(2.0)], TFillXY()),
            true
        )
    ), [])
    |> ignore
}