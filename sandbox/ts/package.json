{
  "name": "flow-ts-integration-test",
  "version": "1.0.0",
  "description": "Test case for Flow9 TypeScript integration with bundling and minification",
  "main": "dist/bundle.js",
  "scripts": {
    "build": "npm run clean && npm run compile && npm run bundle && npm run minify",
    "clean": "rm -rf dist",
    "compile": "tsc",
    "bundle": "webpack --mode production",
    "minify": "terser dist/bundle.js -o dist/bundle.min.js --compress --mangle",
    "dev": "webpack --mode development --watch",
    "start": "node dist/bundle.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "terser": "^5.0.0",
    "ts-loader": "^9.0.0",
    "typescript": "^5.0.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^5.0.0"
  },
  "dependencies": {},
  "keywords": ["flow9", "typescript", "integration", "bundling", "minification"],
  "author": "Flow9 Team",
  "license": "MIT"
}