import ds/array;
import runtime;

maxInteger = 2147483647

findSmallest(array : [int]) -> int {
    fold(array, maxInteger, min);
}

main() {
    println(findSmallest([15, 5, 8, 2, 5]));
    a = [15, 5, 8, 2, 5];
    b = [13, 4, 3, 7, 8];
    println(findSmallest(concat(a,b))==min(findSmallest(a), findSmallest(b)));
}