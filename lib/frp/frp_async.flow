///  rAsyncSelectV ///

rAsyncSelectV(
	src : FrpValue<?>,
	fn : (?) -> ??,
	errorFn : (?) -> string,
) -> FrpAsyncValue<??> {
	v = rget(src);
	err = errorFn(v);
	dst = makeFrpAsyncValue(fn(v));
	if (err != "") rAsyncSetError(dst, err);
	rAsyncConnectV(src, dst, false, fn, errorFn);
	dst;
}

rAsyncSelectV2(
	src1 : FrpValue<?>,
	src2 : FrpValue<??>,
