/// select ///

rAsyncSelect(
	src : FrpAsyncValue<?>,
	fn : (?) -> ??,
	errorFn : ([string]) -> [string]
) -> FrpAsyncValue<??> {
	dst = makeFrpAsyncValue(fn(rAsyncGet(src)));
	rAsyncConnect(src, dst, true, fn, errorFn);
	dst;
}

rAsyncSelect2(
	src1 : FrpAsyncValue<?>,
	src2 : FrpAsyncValue<??>,
	fn : (?, ??) -> ???,
	errorFn : ([string]) -> [string]
) -> FrpAsyncValue<???> {
	dst = makeFrpAsyncValue(fn(rAsyncGet(src1), rAsyncGet(src2)));
	rAsyncConnect2(src1, src2, dst, true, fn, errorFn);
	dst;
}

