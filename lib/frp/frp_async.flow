rAsyncConnectV(
	src : FrpValue<?>,
	dst : FrpAsyncValue<??>,
	init : bool,
	fn : (?) -> ??,
	errorFn : (?) -> string,
) -> void {

	uns = rsubscribe(src, init, \v -> {
		err = errorFn(v);
		if (err == "") rAsyncNext(dst, fn(v)) else rAsyncSetError(dst, err);
	});
	rAddDisposer(dst.frpNode, uns);
}

rAsyncConnectV2(
