	init : bool,
	fn : (?, ??) -> ???,
	errorFn : (?, ??) -> string,
) -> void {
	uns = rsubscribe2(src1, src2, init, \v1,v2 -> {
		err = errorFn(v1, v2);
		if (err == "") rAsyncNext(dst, fn(v1, v2)) else rAsyncSetError(dst, err);
