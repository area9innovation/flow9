
// Implementation using a loop
euler1Loop(limit : i32) -> i32 {
	start = 1;
	acc = 0;
	loop {
		break_if<1>(acc, start >= limit);
		if (start % 3 == 0 | start % 5 == 0) {
			acc := acc + start;
		};
		start := start + 1;
		// This is really continue inside the loop
		break<>();
	};
	// This is never reached, but we have to get this to return i32
	acc;
}
