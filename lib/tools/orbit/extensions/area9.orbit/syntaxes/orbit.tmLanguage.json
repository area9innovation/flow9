{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Orbit",
	"patterns": [
		{ "include": "#math_expr" },
		{ "include": "#id" },
		{ "include": "#string" },
		{ "include": "#" },
		{ "include": "#" },
		{ "include": "#greek_letter" },
		{ "include": "#uid" },
		{ "include": "#linecomment" },
		{ "include": "#blockcomment" }
	],
	"repository": {
		"math_expr": {
			"patterns": [{
				"name": "keyword.control",
				"match": "\\b(Q8|Qâ‚ˆ|Z/|Zn|alpha|beta|c=|chi|d/d|delta|else|epsilon|eta|exists|false|fn|forall|gamma|if|import|in|intersect|iota|is|kappa|lambda|let|mu|nabla|nu|omega|omicron|phi|pi|psi|rho|sigma|tau|then|theta|true|union|upsilon|xi|zeta)\\b"
			}]
		},
		"id": {
			"patterns": [{
				"name": "variable.parameter",
				"match": "[a-z_][a-zA-Z_\\d]*"
			}]
		},
		"string": {
			"name": "string.quoted.double",
			"begin": "\"",
			"end": "\""

		}
		,
		"": {
			"patterns": [{
				"name": "constant.numeric",
				"match": ""
			}]
		},
		"": {
			"patterns": [{
				"name": "constant.numeric",
				"match": ""
			}]
		},
		"greek_letter": {
			"patterns": [{
				"name": "constant.other.symbol",
				"match": "(alpha[^[a-zA-Z_\\d]])|((\\x{000003B1})|((beta[^[a-zA-Z_\\d]])|((\\x{000003B2})|((gamma[^[a-zA-Z_\\d]])|((\\x{000003B3})|((delta[^[a-zA-Z_\\d]])|((\\x{000003B4})|((epsilon[^[a-zA-Z_\\d]])|((\\x{000003B5})|((zeta[^[a-zA-Z_\\d]])|((\\x{000003B6})|((eta[^[a-zA-Z_\\d]])|((\\x{000003B7})|((theta[^[a-zA-Z_\\d]])|((\\x{000003B8})|((iota[^[a-zA-Z_\\d]])|((\\x{000003B9})|((kappa[^[a-zA-Z_\\d]])|((\\x{000003BA})|((lambda[^[a-zA-Z_\\d]])|((\\x{000003BB})|((mu[^[a-zA-Z_\\d]])|((\\x{000003BC})|((nu[^[a-zA-Z_\\d]])|((\\x{000003BD})|((xi[^[a-zA-Z_\\d]])|((\\x{000003BE})|((omicron[^[a-zA-Z_\\d]])|((\\x{000003BF})|((pi[^[a-zA-Z_\\d]])|((\\x{000003C0})|((rho[^[a-zA-Z_\\d]])|((\\x{000003C1})|((sigma[^[a-zA-Z_\\d]])|((\\x{000003C3})|((tau[^[a-zA-Z_\\d]])|((\\x{000003C4})|((upsilon[^[a-zA-Z_\\d]])|((\\x{000003C5})|((phi[^[a-zA-Z_\\d]])|((\\x{000003C6})|((chi[^[a-zA-Z_\\d]])|((\\x{000003C7})|((psi[^[a-zA-Z_\\d]])|((\\x{000003C8})|((omega[^[a-zA-Z_\\d]])|((\\x{000003C9})|((Q8[^[a-zA-Z_\\d]])|((Q\\x{00002088})|((Z[^[a-zA-Z_\\d]])|((\\x{00002124})|((Q[^[a-zA-Z_\\d]])|((\\x{0000211A})|((R[^[a-zA-Z_\\d]])|((\\x{0000211D})|((C[^[a-zA-Z_\\d]])|((\\x{00002102})|((N[^[a-zA-Z_\\d]])|((\\x{00002115})|((Zn)|((\\x{00002124}\\x{00002099})|(([Z/\\x{00002124}/]\\d+[Z\\x{00002124}])|((nabla[^[a-zA-Z_\\d]])|(\\x{00002207}))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))"
			}]
		},
		"uid": {
			"patterns": [{
				"name": "entity.name.function",
				"match": "[A-Z][a-zA-Z_\\d]*"
			}]
		},
		"linecomment": {
			"patterns": [{
				"name": "comment.line",
				"match": "//.*$\n?"
			}]
		},
		"blockcomment": {
			"name": "comment.block",
			"begin": "/\\*",
			"end": "\\*/"

		}

	},
	"scopeName": "source.orbit"
}
