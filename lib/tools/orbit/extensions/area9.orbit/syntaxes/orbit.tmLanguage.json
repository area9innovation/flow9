{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Orbit",
	"patterns": [
		{ "include": "#math_expr" },
		{ "include": "#variable_atom" },
		{ "include": "#constructor_atom" },
		{ "include": "#string" },
		{ "include": "#signed_int" },
		{ "include": "#double" },
		{ "include": "#operator" },
		{ "include": "#linecomment" },
		{ "include": "#blockcomment" }
	],
	"repository": {
		"math_expr": {
			"patterns": [{
				"name": "keyword.control",
				"match": "\\b(c=|d/d|else|false|fn|if|import|in|intersect|is|let|then|true|union)\\b"
			}]
		},
		"variable_atom": {
			"patterns": [{
				"name": "variable.parameter",
				"match": "[a-z_\\x{000003B1}\\x{000003B2}\\x{000003B3}\\x{000003B4}\\x{000003B5}\\x{000003B6}\\x{000003B7}\\x{000003B8}\\x{000003B9}\\x{000003BA}\\x{000003BB}\\x{000003BC}\\x{000003BD}\\x{000003BE}\\x{000003BF}\\x{000003C0}\\x{000003C1}\\x{000003C3}\\x{000003C4}\\x{000003C5}\\x{000003C6}\\x{000003C7}\\x{000003C8}\\x{000003C9}\\x{000003C2}\\x{00002207}][a-zA-Z\\d_'\\x{000003B1}\\x{000003B2}\\x{000003B3}\\x{000003B4}\\x{000003B5}\\x{000003B6}\\x{000003B7}\\x{000003B8}\\x{000003B9}\\x{000003BA}\\x{000003BB}\\x{000003BC}\\x{000003BD}\\x{000003BE}\\x{000003BF}\\x{000003C0}\\x{000003C1}\\x{000003C3}\\x{000003C4}\\x{000003C5}\\x{000003C6}\\x{000003C7}\\x{000003C8}\\x{000003C9}\\x{000003C2}\\x{00002207}\\x{00000391}\\x{00000392}\\x{00000393}\\x{00000394}\\x{00000395}\\x{00000396}\\x{00000397}\\x{00000398}\\x{00000399}\\x{0000039A}\\x{0000039B}\\x{0000039C}\\x{0000039D}\\x{0000039E}\\x{0000039F}\\x{000003A0}\\x{000003A1}\\x{000003A3}\\x{000003A4}\\x{000003A5}\\x{000003A6}\\x{000003A7}\\x{000003A8}\\x{000003A9}\\x{00002124}\\x{0000211A}\\x{0000211D}\\x{00002102}\\x{00002115}]*"
			}]
		},
		"constructor_atom": {
			"patterns": [{
				"name": "entity.name.function",
				"match": "[A-Z\\x{00000391}\\x{00000392}\\x{00000393}\\x{00000394}\\x{00000395}\\x{00000396}\\x{00000397}\\x{00000398}\\x{00000399}\\x{0000039A}\\x{0000039B}\\x{0000039C}\\x{0000039D}\\x{0000039E}\\x{0000039F}\\x{000003A0}\\x{000003A1}\\x{000003A3}\\x{000003A4}\\x{000003A5}\\x{000003A6}\\x{000003A7}\\x{000003A8}\\x{000003A9}\\x{00002124}\\x{0000211A}\\x{0000211D}\\x{00002102}\\x{00002115}][a-zA-Z\\d_'\\x{000003B1}\\x{000003B2}\\x{000003B3}\\x{000003B4}\\x{000003B5}\\x{000003B6}\\x{000003B7}\\x{000003B8}\\x{000003B9}\\x{000003BA}\\x{000003BB}\\x{000003BC}\\x{000003BD}\\x{000003BE}\\x{000003BF}\\x{000003C0}\\x{000003C1}\\x{000003C3}\\x{000003C4}\\x{000003C5}\\x{000003C6}\\x{000003C7}\\x{000003C8}\\x{000003C9}\\x{000003C2}\\x{00002207}\\x{00000391}\\x{00000392}\\x{00000393}\\x{00000394}\\x{00000395}\\x{00000396}\\x{00000397}\\x{00000398}\\x{00000399}\\x{0000039A}\\x{0000039B}\\x{0000039C}\\x{0000039D}\\x{0000039E}\\x{0000039F}\\x{000003A0}\\x{000003A1}\\x{000003A3}\\x{000003A4}\\x{000003A5}\\x{000003A6}\\x{000003A7}\\x{000003A8}\\x{000003A9}\\x{00002124}\\x{0000211A}\\x{0000211D}\\x{00002102}\\x{00002115}]*"
			}]
		},
		"string": {
			"name": "string.quoted.double",
			"begin": "\"",
			"end": "\""

		}
		,
		"signed_int": {
			"patterns": [{
				"name": "constant.numeric",
				"match": "\\-?\\d+"
			}]
		},
		"double": {
			"patterns": [{
				"name": "constant.numeric",
				"match": "(\\-?\\d+(\\.\\d*([Ee][\\+\\-]?\\d+)?)|([Ee][\\+\\-]?\\d+))|(\\.\\d+([Ee][\\+\\-]?\\d+)?)"
			}]
		},
		"operator": {
			"patterns": [{
				"name": "constant.other.symbol",
				"match": "[\\+\\-\\*·/%×\\x{000022CA}\\x{000022C9}\\x{00002218}\\^\\x{00002297}\\&\\&\\x{00002227}\\|\\|\\x{00002228}!¬===!=<><=\\x{00002264}>=\\x{00002265}\\x{00002282}c=\\x{00002208}in\\x{0000222A}\\x{00002229}\\x{000022B2}\\x{00002245}\\x{00002192}\\->:!:\\x{000021D2}=>\\x{000021D4}<=>\\x{000022A2}\\|\\-;\\.\\\\x{0000222B}\\x{00002211}\\x{00002202}\\|\\x{000003C6}\\\\\\x{000003BB}]"
			}]
		},
		"linecomment": {
			"patterns": [{
				"name": "comment.line",
				"match": "//.*$\n?"
			}]
		},
		"blockcomment": {
			"name": "comment.block",
			"begin": "/\\*",
			"end": "\\*/"

		}

	},
	"scopeName": "source.orbit"
}
