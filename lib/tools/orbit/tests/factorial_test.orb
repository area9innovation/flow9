// Simple factorial test

// Define a factorial function
fn factorial(n) = n is (
  0 => 1;
  1 => 1;
  n => n * factorial(n - 1)
);

// Calculate some factorials
//println("Factorial of 0: " + i2s(factorial(0)));
//println("Factorial of 1: " + i2s(factorial(1)));
println("Factorial of 5: " + i2s(factorial(5)));