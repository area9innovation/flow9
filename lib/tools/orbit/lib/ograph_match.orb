// A simple test for S-expression pattern matching in OGraph

println("Pattern matching test");

// Create a new graph for OGraph operations
let graph = makeOGraph("graph");
println("Creating expression (a && b) in the graph");

// Create the nodes for our test expression
let exprId = addOGraph(graph, '(a && b));

// Call the pattern matcher
matchOGraphPattern(graph, '`&&`(args), \(bindings, eclassId) -> (
    println("MATCH FOUND!");
    println("Bindings: " + bindings);
    println("Matched at eclass ID: " + i2s(eclassId));
));

println("Done with test");
