lookupTree(a0:Tree<?, ??>, a1:?) -> Maybe<??> {None()}

fiType2DTypeInst(program : FiProgram) -> string {
	switch (lookupTree(program.names.structs, "")) {
		Some(st): {
			switch (st.args[0].type) {
				FiTypeParameter(__): "";
				default: "";
			}
		}
		None(): {
			"STRUCT ERROR"; 
		}
	}
}
