addValues(x : int, y : int) -> int {
    x + y
}

addDoubles(x : double, y : double) -> double {
    x + y
}

addStrings(x : string, y : string) -> string {
    x + y
}

testOverloadedAddition() {
    intResult = addValues(5, 10);
    doubleResult = addDoubles(2.5, 3.5);
    stringResult = addStrings("Good ", "Morning!");
	[1, 2, 3]
}
