csv = row:r1 (nl row)*:rs { consArray(:r1, :rs) };
row = {#}:p cell:c1 (sp cell)*:cs { CsvRowInfo(:p, #, consArray(:c1, :cs)) }; 
cell = ws* escaped$s1 ws* { unescapeQ($s1) } | plain$s2 { $s2 };
escaped = (q (!q char)* q)+;
plain = (!(nl | sp | q) char)*;
char = '\u0000'-'\uFFFD';
ws = "%3" | "%4";
nl = "\r"? "\n";
q = "%2";
sp = "%1";
