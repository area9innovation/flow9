lspCompletionItem2Json(item : LspCompletionItem) -> Json {
	edit2json = \edit -> switch (edit) {
		LspTextEdit(__,__): lspTextEdit2Json(edit);
		LspInsertReplaceEdit(__,__,__): lspInsertReplaceEdit2json(edit);
	}
	JsonObject(filtermap([
		Some(Pair("label", JsonString(item.label))),
		if (item.labelDetails.detail == "" && item.labelDetails.description == "") None() else Some(Pair("labelDetails", lspCompletionItemLabelDetails2Json(item.labelDetails))),
		if (item.kind == 0) None() else Some(Pair("kind", JsonDouble(i2d(item.kind)))),
		if (item.tags == []) None() else Some(Pair("tags", JsonArray(map(item.tags, \tag -> JsonString(i2s(tag)))))),
		if (item.detail == "") None() else Some(Pair("detail", JsonString(item.detail))),
		if (item.documentation == "") None() else Some(Pair("documentation", JsonString(item.documentation))),
		if (!item.deprecated) None() else Some(Pair("deprecated", JsonBool(item.deprecated))),
		if (!item.preselect) None() else Some(Pair("preselect", JsonBool(item.preselect))),
		if (item.sortText == "") None() else Some(Pair("sortText", JsonString(item.sortText))),
		if (item.filterText == "") None() else Some(Pair("filterText", JsonString(item.filterText))),
		if (item.insertText == "") None() else Some(Pair("insertText", JsonString(item.insertText))),
		if (item.insertTextFormat == 0) None() else Some(Pair("insertTextFormat", JsonDouble(i2d(item.insertTextFormat)))),
		if (item.insertTextMode == 0) None() else Some(Pair("insertTextMode", JsonDouble(i2d(item.insertTextMode)))),
		maybeMap(item.textEdit, \edit -> Pair("textEdit", edit2json(edit))),
		if (item.additionalTextEdits == []) None() else Some(Pair("additionalTextEdits", JsonArray(map(item.additionalTextEdits, lspTextEdit2Json)))),
		if (item.commitCharacters == []) None() else Some(Pair("commitCharacters", JsonArray(map(item.commitCharacters, \ch -> JsonString(ch))))),
		maybeMap(item.command, \command -> Pair("command", lspCommand2Json(command))),
		if (item.data == JsonNull()) None() else Some(Pair("data", item.data))
	], idfn));
}

