import ds/treeutils;

export {
	svgName2color(name : string, defaultReturn : int) -> int;
}

svgName2color(name : string, defaultReturn : int) -> int {
	lookupTreeDef(svgColorNames, name, Pair("", defaultReturn)).second
}

