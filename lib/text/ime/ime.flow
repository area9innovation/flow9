export {
	InputMethodAPI(
		getPredictions : (context : string, current : string, packId : Maybe<?>, count : int) -> InputMethodPredictionChoices<?>
	);

	InputMethodPredictionChoices(
		previousId : Maybe<?>,
		nextId : Maybe<?>,
		choices : [InputMethodPredictionChoice]
	);

	InputMethodPredictionChoice(
		text : string,
		hotkey : string,
		caption : string
	);

	InputMethodBrowser(
		api : InputMethodAPI<?>,
		context : string,
		input : string,
		pageSize : int,
		choicesB : DynamicBehaviour<InputMethodPredictionChoices<?>>,
		inPageIdxB : DynamicBehaviour<int>
	);

	makeIMEBrowser(api : InputMethodAPI, context : string, input : string, pageSize : int) -> InputMethodBrowser<?>;
	goPreviousPageIME(b : InputMethodBrowser<?>) -> bool;
	goNextPageIME(b : InputMethodBrowser<?>) -> bool;
	goPreviousCandidateIME(b : InputMethodBrowser<?>) -> bool;
	goNextCandidateIME(b : InputMethodBrowser<?>) -> bool;

	getTestPinyin(lang : string) -> InputMethodAPI<int>;
}

makeIMEBrowser(api : InputMethodAPI, context : string, input : string, pageSize : int) -> InputMethodBrowser<?> {
	InputMethodBrowser(api, context, input, pageSize, make(api.getPredictions(context, input, None(), pageSize)), make(0))
}

goPreviousPageIME(b : InputMethodBrowser<?>) {
	nextDistinct(b.choicesB, b.api.getPredictions(b.context, b.input, getValue(b.choicesB).previousId, b.pageSize));
	true
}

goNextPageIME(b : InputMethodBrowser<?>) {
	nextDistinct(b.choicesB, b.api.getPredictions(b.context, b.input, getValue(b.choicesB).nextId, b.pageSize));
	true
}

goPreviousCandidateIME(b : InputMethodBrowser<?>) {
	lastIdx = length(getValue(b.choicesB).choices)-1;
	currentPredictionIdx = max(0, min(lastIdx, getValue(b.inPageIdxB)));
	if (currentPredictionIdx > 0) {
		nextDistinct(b.inPageIdxB, currentPredictionIdx-1);
		true
	} else {
		r = goPreviousPageIME(b);
		nextDistinct(b.inPageIdxB, if (r) length(getValue(b.choicesB).choices)-1 else 0);
		r
	}
}

goNextCandidateIME(b : InputMethodBrowser<?>) {
	lastIdx = length(getValue(b.choicesB).choices)-1;
	currentPredictionIdx = getValue(b.inPageIdxB);
	if (currentPredictionIdx < lastIdx) {
		nextDistinct(b.inPageIdxB, currentPredictionIdx+1);
		true
	} else {
		r = goNextPageIME(b);
		nextDistinct(b.inPageIdxB, if (r) 0 else lastIdx);
		r
	}
};

