intOr = \a, b -> if (a != 0) a else b;

getSubset(set : [Pair<string, [?]>], prefix : string, needOthers : bool) -> [Pair<string, [?]>] {
	prefixLen = strlen(prefix);

	expandKey = \i, a, t -> if (i < 0) Triple(bitNot(i), t, a) else Triple(i, t, arrayPush(a, set[i]));
	setCmp = \d : Pair<string, [?]>, k : string -> {
		intOr(strlen(d.first) - strlen(k), genericCompare(d.first, k));
	};

	setLast = length(set)-1;
	maxLen = strlen(lastElement(set, Pair("", [])).first);
	mainKey = expandKey(binarySearch2(set, prefix, setCmp), [], prefix);

	// Collecting possible strings that start from what «prefix» contains.
	keys = updateWhile(mainKey, \keyTextKeys -> if (keyTextKeys.first > setLast) None() else {
		key = keyTextKeys.first;
		keyText : string = keyTextKeys.second+fromCharCode(1);
		keys = keyTextKeys.third;
		// TODO maybe prepare a map at which index each key length starts
		// to optimize the search.
		nextKey = expandKey(binarySearchFull(set, keyText, key+1, setLast, setCmp, -1, true), keys, keyText);
		// Collecting possible strings of same length
		Some(updateWhile(nextKey, \nkeyTextKeys -> if (nkeyTextKeys.first > setLast) None() else {
			nsetElem = set[nkeyTextKeys.first];
			if (startsWith(nsetElem.first, prefix)) Some(Triple(
				nkeyTextKeys.first+1, nkeyTextKeys.second, arrayPush(nkeyTextKeys.third, nsetElem)
			)) else None() // We expect them to be in a single piece.
		}))
	});
	(if (needOthers)
		// Linear, since a few items suggested when «needOthers».
		updateWhile(Triple(mainKey.first-1, prefix, keys.third), \keyTextKeys -> if (keyTextKeys.first < 0) None() else {
			key = keyTextKeys.first;
			setElem = set[keyTextKeys.first];
			elemText = setElem.first;
			Some(Triple(
				key-1, elemText,
				if (startsWith(prefix, elemText))
					arrayPush(keyTextKeys.third, setElem)
				else keyTextKeys.third
			))
		})
	else keys).third;
};

getTestPinyin(lang : string) -> InputMethodAPI<int> {
	cache : ref Triple<string, string, [Pair<string, string>]> = ref Triple("", "", []);
	InputMethodAPI(
		\context : string, current : string, packId : Maybe<int>, count : int -> {
			currentLen = strlen(current);
			if ((^cache).first != context || (^cache).second != current) {
				cache := Triple(context, current, {
					reverseContext = reverse(context);
					langData : [Pair<string, [Pair<string, [Pair<string, int>]>]>] = findDef(testInputMethodPinyinData, \d -> d.first == lang, Pair(lang, [])).second;
					candidates : [Pair<Pair<string, string>, [int]>] = tree2pairs(fold(getSubset(langData, current, false), makeTree(), \keyAcc : Tree<Pair<string, string>, [int]>, key : Pair<string, [Pair<string, [Pair<string, int>]>]> -> {
						keylen = strlen(key.first);
						fold(getSubset(key.second, reverseContext, true), keyAcc, \contextCandidateAcc : Tree<Pair<string, string>, [int]>, contextCandidate : Pair<string, [Pair<string, int>]> -> {
							fold(contextCandidate.second, contextCandidateAcc, \insertionAcc : Tree<Pair<string, string>, [int]>, insertion : Pair<string, int> -> {
								treeKey = Pair(insertion.first, key.first);
								oldValue = lookupTreeDef(insertionAcc, treeKey, [0, 0, 0]);
								// Listing exact length match first, then superstrings, and substrings last.
								newValue = if (keylen == currentLen)
									[oldValue[0]+insertion.second, oldValue[1], oldValue[2]]
								else if (keylen > currentLen)
									[oldValue[0], oldValue[1]+insertion.second, oldValue[2]]
								else
									[oldValue[0], oldValue[1], oldValue[2]+insertion.second];
								setTree(insertionAcc, treeKey, newValue)
							})
						});
					}));
					map(mergeSort(candidates, \ap, bp -> {
						a = ap.second;
						b = bp.second;
						intOr(b[0]-a[0], intOr(b[1]-a[1], b[2]-a[2]))
					}), firstOfPair)
				});
			};
			start = either(packId, 0);
			cacheLen = length((^cache).third);
			InputMethodPredictionChoices(
				Some(if (start < count) (cacheLen-1) |> \v -> v - v % count else start - count),
				Some(if (start + count >= cacheLen) 0 else start + count),
				map(
					subrange((^cache).third, start, count),
					\s -> InputMethodPredictionChoice(s.first, "", s.second)
				)
			)
		}
	)
}

