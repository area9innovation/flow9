		MGetFocusGroup(\p -> MGetMaterial2Tropic(\m2t -> TBorderA(start, top, end, bottom, m2t(m, p))))
	}

	MPadA(x : double, y : double, content : Material) -> Material {
		if (getDefaultRtl()) MBorder(0.0, y, x, 0.0, content) else MPad(x, y, content)
	};

	extractCommonAlignmentFromMParagraphStyles(styles: [MParagraphStyle]) -> CommonAlignment;
}

extractCommonAlignmentFromMParagraphStyles(styles: [MParagraphStyle]) -> CommonAlignment {
	fold(styles, StartAlign(), \acc, style ->
		switch (style) {
			EndAlign(): style;
			LeftAlign(): style;
			RightAlign(): style;
			CenterAlign(): style;
			default: acc;
		}
	)
}