		RMSmoothIntersection2(object1, object2, k): concat(extractRMTextFromRMScene(object1), extractRMTextFromRMScene(object2));
		RMSmoothIntersection(objects, k): mapConcat(objects, extractRMTextFromRMScene);
		RMSmoothSubtraction2(object1, object2, k): concat(extractRMTextFromRMScene(object1), extractRMTextFromRMScene(object2));
		RMSmoothSubtraction(objects, k): mapConcat(objects, extractRMTextFromRMScene);
		RMMaterial(object, color, materialProperties): extractRMTextFromRMScene(object);
		RMInteractive(object, trigger): extractRMTextFromRMScene(object);
		RMText(text, position, showDot): [RMText(text, position, showDot)];
		RMMesh(mesh, position, rotation, scale, color): [];
		RMEmpty(): [];
	}
}

extractRMMeshFromRMScene(obj : RMScene) -> [RMMesh] {
	switch(obj) {
		RMCompositeObject(object, name, color, materialProperties, position, rotation, scale, trigger, shown): [];
		RMPrimitive(): [];
		RMTranslate(object, d): extractRMMeshFromRMScene(object);
		RMRotate(object, r): extractRMMeshFromRMScene(object);
		RMScale(object, s): extractRMMeshFromRMScene(object);
		RMRepetitionLimited(object, c, l): extractRMMeshFromRMScene(object);
		RM3DRound(object, radius): extractRMMeshFromRMScene(object);
		RMUnion2(object1, object2): concat(extractRMMeshFromRMScene(object1), extractRMMeshFromRMScene(object2));
		RMUnion(objects): mapConcat(objects, extractRMMeshFromRMScene);
		RMIntersection2(object1, object2): concat(extractRMMeshFromRMScene(object1), extractRMMeshFromRMScene(object2));
		RMIntersection(objects): mapConcat(objects, extractRMMeshFromRMScene);
		RMSubtraction2(object1, object2): concat(extractRMMeshFromRMScene(object1), extractRMMeshFromRMScene(object2));
		RMSubtraction(objects): mapConcat(objects, extractRMMeshFromRMScene);
		RMSmoothUnion2(object1, object2, k): concat(extractRMMeshFromRMScene(object1), extractRMMeshFromRMScene(object2));
		RMSmoothUnion(objects, k): mapConcat(objects, extractRMMeshFromRMScene);
		RMSmoothIntersection2(object1, object2, k): concat(extractRMMeshFromRMScene(object1), extractRMMeshFromRMScene(object2));
		RMSmoothIntersection(objects, k): mapConcat(objects, extractRMMeshFromRMScene);
		RMSmoothSubtraction2(object1, object2, k): concat(extractRMMeshFromRMScene(object1), extractRMMeshFromRMScene(object2));
		RMSmoothSubtraction(objects, k): mapConcat(objects, extractRMMeshFromRMScene);
		RMMaterial(object, color, materialProperties): extractRMMeshFromRMScene(object);
		RMInteractive(object, trigger): extractRMMeshFromRMScene(object);
		RMText(text, position, showDot): [];
		RMMesh(mesh, position, rotation, scale, color): [RMMesh(mesh, position, rotation, scale, color)];
		RMEmpty(): [];
	}
}

makeRaymarchingMaterialTexture(material : Material, texture : DynamicBehaviour<string>) -> Material {
	isDisplayed = make(true);
	pngFn = make(None());
	MConstruct(
		[
			makeSubscribe(pngFn, \pfn -> {
				maybeApply(
					pfn,
					\fn -> fn(\v -> {
						timer(100,\ -> next(isDisplayed, false));
						next(texture, v);
					})
				)
			})
		],
		MExtractPNG(
			MIf(
				isDisplayed,
				material,
				MEmpty()
			),
			\fn -> next(pngFn, fn)
		)
	)
}

rmiTree2GLSL(rmiScene : Tree<int, RMIExp>) -> string {
	strGlue(
		convertTreeToArray(
			rmiScene,
			\k, v -> formatString("if (id == %1) {\nresult = %2;\n}", [i2s(k), RMIExp2GLSL(v)])
		),
		" else "
	);
}

getRMTextFontSize(textFontSettings : RMTextFontSettings, distance : double) -> double {
	baseSize = textFontSettings.minSize - textFontSettings.k1 * log((distance - textFontSettings.k2)/ textFontSettings.k3);
	if (baseSize < textFontSettings.minSize && distance > textFontSettings.k2) {
		textFontSettings.minSize
	} else if (baseSize > textFontSettings.maxSize || distance < textFontSettings.k2) {
		textFontSettings.maxSize
	} else {
		baseSize
	};
}

getTextCoords(id : int, textPositionsB : DynamicBehaviour<[Triple<Point, double, bool>]>) {
	timer(10, \ -> {
		textCoords = map(hostCall("getTextCoords", [flow(i2s(id))]), parseJson);

		textPositions = map(
			textCoords,
			\tp -> {
				Triple(
					Point(getJsonDoubleField(tp, "x", 0.), getJsonDoubleField(tp, "y", 0.)),
					getJsonDoubleField(tp, "distance", 0.),
					getJsonBoolField(tp, "inBounds", false)
				)
			}
		);
		if (textPositions != []) nextDistinct(textPositionsB, textPositions);

		getTextCoords(id, textPositionsB);
	})
}

generatePolygonFunctions(polygonPoints : Set<int>) -> string {
	template = "float sdPolygon(vec2[%1] v, vec2 p)
	{
		float d = dot(p - v[0], p - v[0]);
		float s = 1.0;
		for(int i = 0, j = %1 - 1; i < %1; j = i, i++)
		{
			vec2 e = v[j] - v[i];
			vec2 w = p - v[i];
			vec2 b = w - e * clamp(dot(w, e) / dot(e, e), 0.0, 1.0);
			d = min(d, dot(b, b));
			bvec3 c = bvec3(p.y >= v[i].y, p.y < v[j].y, e.x * w.y > e.y * w.x);
			if(all(c) || all(not(c))) s *= -1.0;  
		}
		return s * sqrt(d);
	}";

	foldSet(polygonPoints, "", \acc, num -> {
		acc + "\n\n" + formatString(template, [i2s(num)]);
	})
}

parseObj(str : string) -> Triple<[double], [double], [double]> {
	lines = strSplit(str, "\n");

	parsed = fold(
		lines,
		Quadruple(
			[],//v
			[],//vt
			[],//vn
			Triple([], [], [])//f
		),
		\acc, line -> {
			segments = strSplit(line, " ");
			if (segments[0] == "v") {
				Quadruple(acc with first = arrayPush(acc.first, [s2d(segments[1]), s2d(segments[2]), s2d(segments[3])]))
			} else if (segments[0] == "vt") {
				Quadruple(acc with second = arrayPush(acc.second, [s2d(segments[1]), s2d(segments[2])]))
			} else if (segments[0] == "vn") {
				Quadruple(acc with third = arrayPush(acc.third, [s2d(segments[1]), s2d(segments[2]), s2d(segments[3])]))
			} else if (segments[0] == "f") {
				if (length(segments) > 3) {
					processedFaces : [Triple<Maybe<[double]>, Maybe<[double]>, Maybe<[double]>>] = map(
						tail(segments),
						\seg -> {
							f = strSplit(seg, "/");
							v = Some(acc.first[s2i(f[0]) - 1]);
							vt = elementAtMap(f, 1, \vti -> if (vti != "") Some(acc.second[s2i(vti) - 1]) else None(), None());
							vn = elementAtMap(f, 2, \vni -> Some(acc.third[s2i(vni) - 1]), None());
							Triple(v, vt, vn)
						}
					);
					createFaces = \fn -> concatA(filtermap(concatA(generate(
						2,
						length(processedFaces),
						\i -> [fn(processedFaces[0]), fn(processedFaces[i - 1]), fn(processedFaces[i])]
					)), idfn));
					Quadruple(acc with fourth = Triple(
						concat(acc.fourth.first, createFaces(firstOfTriple)),
						concat(acc.fourth.second, createFaces(secondOfTriple)),
						concat(acc.fourth.third, createFaces(thirdOfTriple))
					))
				} else acc;
			} else acc;
		}
	)

	parsed.fourth
}