generateAreaSizeInfoA(state : MDockingState) -> [MDockingAreaSizeInfo] {
	println("--------------------------------");
	println("------extractAreaSizeInfo-------");
	println("--------------------------------");

	dockA = getValue(state.dock);
	areaIdsA = uniq(map(dockA,\v -> firstElement(v, intMax)));
	println("Area Ids: " + toString(areaIdsA));

	out = 	map(areaIdsA, \aId -> {
		
			aGrid = extractAreaGridById(dockA, aId);

			println(aId);
			println(aGrid);

			makeAreaSizeInfoFromGrid(state, aGrid, aId);

		});

	iter(out, \o -> println(o));
	out 
}


makeAreaSizeInfoFromGrid(state : MDockingState, aGrid : [[int]], aId :int) -> MDockingAreaSizeInfo {
	MDockingAreaSizeInfo(
		aId, 
		[],
		make(map(aGrid, \colA -> {
			
			panelsWidthA = map(colA, \s -> getTropicMetrics(getValue(state.size)[s]).width);
			// dockedWidthA = map(colA, \s -> getTropicMetrics(getValue(state.dockedSize)[s]).width);
			// mobileWidthA = map(colA, \s -> getTropicMetrics(getValue(state.mobileSize)[s]).width);

			maxPanelsWidth = either(maxA(panelsWidthA), -doubleMax);
			// maxDockedWidth = either(maxA(dockedWidthA), -doubleMax);
			// maxMobileWidth = either(maxA(mobileWidthA), -doubleMax);

			maxPanelsWidth;
			
		})),
		make(map(aGrid, \colA -> map(colA, \panelId -> {
			getTropicMetrics(getValue(state.size)[panelId]).height;
		})))
	);
}

printArea(areaSizeInfo : MDockingAreaSizeInfo) -> void {

	println(areaSizeInfo.areaId);
	println("cols: " + toString(getValue(areaSizeInfo.colSizeA)));
	println("rows: "+ toString(getValue(areaSizeInfo.rowSizeA)));
	println(" ");
}

	

showIf(cond : bool) -> (Material) -> Material {\m -> if (cond) m else TEmpty()}
makeColSize(s : double, rotate: bool) -> Tropic { if (rotate) TFillWY(s) else TFillXH(s)}
makeRowSize(s : double, rotate: bool) -> Tropic { if (rotate) TFillXH(s) else TFillWY(s)}
makeColFixedSize(w : double, h : double, rotate: bool) -> Tropic { if (rotate) TFixed(w, h) else TFixed(h, w)}
makeRowFixedSize(w : double, h : double, rotate: bool) -> Tropic { if (rotate) TFixed(h, w) else TFixed(w, h)}



updateColSize(areaSizeInfo : MDockingAreaSizeInfo,  colId : int, value : double) -> void {
	updateBehaviourDistinct(areaSizeInfo.colSizeA, \csA -> replace(csA, colId, value));
}

updateRowSize(areaSizeInfo : MDockingAreaSizeInfo,  colId : int, rowId : int, value : double) -> void {
	updateBehaviourDistinct(areaSizeInfo.rowSizeA, \csA -> replace(csA, colId, 
		replace(csA[colId], rowId, value)
	));
}






// Col separator with resize handle and drop area
makeMDockingSeparatorDrop(
	isColumn : bool,
	resizeCol : bool,  // resize cols (W) or panels (H)
	id : int,		   // it could be colId or rowId
	colA : [int], 	   // it could be colA or rowA
	numOfCols : int,
	dropInfo : MDockingDropInfo, 
	resizePressed : DynamicBehaviour<bool>,
	state : MDockingState, 
	style : [MDockingStyle]
) -> Material {

		manager = state.manager;	
		isHovering = make(false);
		isRightCol  = contains(style, MDockingRightCols());
		isBottomLine = contains(style, MDockingBottomtLines());
		dontResize  = contains(style, MDockingDontResize());
		dontResizeAll = fold(colA, true, \acc, p -> acc && contains(getValue(state.style)[p], MDockingDontResize()));


