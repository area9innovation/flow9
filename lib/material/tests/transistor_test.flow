import transforms;
import material/make_subscribe;

main() {
	g = make(true);
	v = make(0);

	tr1 = transistor(g, v);
	tr2 = transistoru(g, v);
	tr3 = ftransistor(g, v);

	makeSubscribe(tr1, \v0 -> println("tr1" + i2s(v0)))();
	makeSubscribe(tr2.value, \v0 -> println("tr2" + i2s(v0)))();
	makeSubscribe(tr3, \v0 -> println("tr3" + i2s(v0)))();

	timer(500, \ -> next(v, 1));
	timer(1000, \ -> next(g, false));
	timer(1500, \ -> next(v, 2));
	timer(2000, \ -> next(v, 3));
	timer(2500, \ -> next(g, true));
	timer(3000, \ -> next(v, 4));
	timer(3500, \ -> next(g, false));
	timer(4000, \ -> next(v, 5));
	timer(4500, \ -> next(g, true));
	timer(4000, \ -> next(v, 6));
	timer(5000, \ -> next(g, false));
}