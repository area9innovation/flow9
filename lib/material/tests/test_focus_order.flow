import material/material2tropic;

main() {
	manager = makeMaterialManager([]);
	setRendererType("html");

	content = MLines([
		MTextButton("BUTTON 1", nop, [], []),
		MTextButton("BUTTON 2", nop, [], []),
		MGetFocusGroup(\p ->
			TForm(
				material2nativeFormInitWithParent(
					manager,
					p,
					MTextButton("FORM BUTTON", nop, [], []) |> (\m -> MDebugConstruct("FORM BUTTON", m))
				)
			)
		),
		MTextButton("BUTTON 3", nop, [], []),
		MTextButton("BUTTON 4", nop, [], [])
	])
	|> (\m -> MComponentGroup(m, [
		AccessRole("group"),
		MFocusId(const(3))
	]));

	mrender(manager, true, content);
}