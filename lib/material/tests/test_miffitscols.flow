import material/material_ui;

main() {
	setRendererType("html");
	manager = makeMaterialManager([]);

	content =
		MBaselineColsA([
			MText("TEXT", []),
			MIfFitsCols(const([
				Pair(MIconButton("android", nop, [], []), Some(1)),
				Pair(MIconButton("arrow_left", nop, [], []), Some(0)),
				Pair(MIconButton("arrow_right", nop, [], []), Some(2)),
			]), None())
			|> MAlignEnd
		]);

	mrender(manager, true, content);
}