import material/material;
import material/material_manager;
import material/material_dialog;

main() {
    manager = makeMaterialManager([]);
    text = make("");
    // fpprint("text")(text);

	contentUp =
		MAutoComplete(text, generate(0, 20, \i -> "content" + i2s(i)), [
			// MShowClearIcon(true, false),
			// MOutlined(),
			// MFilled(),
			// MCustomTextStyle([MDisplay3()]),
			// MMaxHeight(200.),
			// MTextInputOutOfPopup(),
			// MSuggestionsOnTop(),
			// MSuggestionInLine(false),
			// MInputBackgroundStyle([MFill(MGreen(200))], [MFill(MRed(200))]),
			MSuggestionsBackgroundStyle([MFill(MBlue(200))]),
			MWidth(300.)

		], [MInputError(const(Some(Pair("Error", true))), [])])
		// |> MAlignBottom
		// |> MCenterY
		|> (\m -> MBorder4(16., m))
		;

	content =
		MGroup2(
			TFillXY() |> addMBackground(MGrey(300)),
			contentUp 
		);

    mrender(manager, true, content);
}