import material/material_ui;

main() {
	manager = makeMaterialManager([]);

	m =
		MCols([
			MGroup2(MDebugMetrics(green, TFillXH(50.)), TRectangle([Fill(yellow)], TFixed(100., 2.))),
			TRectangle([Fill(cyan)], TFixed(50., 50.)),
			MDebugMetrics(red, TFillXH(50.)),
		]);

	mrender(manager, true, m);
}