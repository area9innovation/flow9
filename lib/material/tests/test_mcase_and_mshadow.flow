import tropic/tropic2form;
import renderform;
import material/material_manager;
main() {
	render(
		tropic2form(
			TCase(
			// Big screen
				[TGroup([ 
					TFillWY(740.0),
					TMutable(fselect(const(1.0), FLift(\x ->
						TRectangle([Fill(1000)], TFillXY()), //blue
					))),
				])],
				// Small screen
				[TRectangle([Fill(32000)], TFillXY()), //green
			]), 
			emptyStylesheet
		)
	);
}
