import material/material2tropic;

main() {
	manager = makeMaterialManager([]);
	content = MCols([
		MDebug(red, MFixed(100., 20.)),
		MDebugMetrics(green, MSlider(make(0.0), [
			MWidth(400.0),
			MSliderIconLeft("android"),
			MSliderIconRight("android"),
			MSliderDisplayValue(true),
			MSliderRange(0., 50.),
			MSliderStep(10.)
		]))
		// |> (\m -> MAvailableWidth(m, const(400.)))
		,
		MDebug(red, MFixed(100., 20.))
	]);

	mrender(manager, true, content);
}