import material/material2tropic;

main() {
	manager = makeMaterialManager([]);

	expander = make(false);

	content =
		TFixed(300., 3000.)
		|> (\t -> TFrame(0., 0., [GradientFill(90., [GradientPoint(green, 1., 0.), GradientPoint(white, 1., 1.)]) ], t))
		|> (\t -> MGroup2(t, MText("text", [])));

	appBar =
		MToolbar(
			expander,
			MText("TITLE", []),
			[],
			[]
		);

	app =
		MAppStructure(
			[
				MContent(content),
				MAppBar(appBar)
			],
			[
				MFloatingToolbar()
			]
		);
	
	mrender(manager, true, app);
}