import material/material2tropic;

main () {

	manager = makeMaterialManager([]);
	dpiScale = extractStruct(manager.manager.renderStyle, TInspectDpiFactor(make(Factor(1.0, 1.0)))).factor;
	stageWH = extractStruct(manager.manager.renderStyle, TInspectStageWH(makeWH())).wh;

	MLines([
		MText("Stage width: " + d2s(getStageWidth()), []),
		MText("Stage height: " + d2s(getStageHeight()), []),
		MText("Screen dpi: " + i2s(screenDPI), []),
		MSelect(dpiScale, \ds ->
			MParagraph("Dpi scale inspected: " + toString(ds), [])
		),
		MSelect(stageWH, \swh ->
			MParagraph("Stage widthheight inspected: " + toString(swh), [])
		)
	])
	|> (\f -> mrender(manager, true, f))
}
