import material/material2tropic;

main() {
	manager = makeMaterialManager([]);

	items = 
		generate(0, 10, \i ->
			TFixed(
				random() * 90. + 10.,
				random() * 90. + 10.
			)
			|> addTBackground(MGreen(50 * (i + 1)))
			|> (\t -> MReorderItem(t, []))
		);

	content =
		MReorderGrid(
			items,
			make([]), [
				MReorderCustomDraggingOrder()
			]
		);
	
	mrender(manager, true, content);
}