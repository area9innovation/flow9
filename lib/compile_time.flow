export {
	// Speculative function, used to make output of computed constant values during compilation.
	// Normally these function is eliminated from program before passing it to backends.
	outputCompileTimeEval(msg: string, v: ?) -> ? {
		v
	}

	// Speculative function, used to forbid a compile-time computation of an exression.
	// Normally these function is eliminated from program before passing it to backends.
	forbidCompileTimeEval(v: ?) -> ? {
		v
	}

	// Speculative function, used to force a compile-time computation of an exression.
	// In case a compile-time computation  fails, the error will be reported
	// Normally these function is eliminated from program before passing it to backends.
	forceCompileTimeEval(v: ?) -> ? {
		v
	}
}
