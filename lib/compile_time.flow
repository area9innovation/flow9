export {
	// This function invokes the interpreter to compute a value during compilation.
	// In case a compile-time computation fails, the error will be reported
	compileTime(v: ?) -> ? {
		v
	}
	// Acts like `compileTime`, but in case a compile-time computation fails, the default value is returned
	compileTimeDef(v: ?, def: ?) -> ? {
		def
	}
}
