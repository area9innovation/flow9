fasync(def : ?, asyncFn : (onDone : (?) -> void) -> void) -> Transform<?> {
	called = ref false;
	t = make(def);

	fselect(t, FLift(\t0 -> {
		if (!^called) {
			called := true;
			asyncFn(\v -> next(t, v));
		}

		getValue(t);
	}));
}

