use_frameworks!
platform :ios, '9.0'

$LOAD_PATH << '.'
require 'BuildPodfile.rb'

target 'bundled' do
	pod 'SocketRocket'
    pod 'GoogleWebRTC', '~> 1.1'
    pod 'Socket.IO-Client-Swift', '~> 15.0.0'
	build_pods
end

target 'generic' do
	pod 'SocketRocket'
    pod 'GoogleWebRTC', '~> 1.1'
    pod 'Socket.IO-Client-Swift', '~> 15.0.0'
end
